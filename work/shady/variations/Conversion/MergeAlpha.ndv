<Shady>
	<Variation desc="Takes RGBA and outputs RGB" simulationCommand="copy">
		<Input type="vec4" name="RGB"/>
		<Input type="float" name="Alpha"/>
		<Output type="vec4" name="RGBA"/>
		<Source type="GLSL">
			<Code output="RGBA">
				[output] = vec4(RGB.xyz, Alpha);
			</Code>
		</Source>
	</Variation>
</Shady>